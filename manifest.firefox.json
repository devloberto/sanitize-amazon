{
  "manifest_version": 2,
  "name": "Sanitize Amazon",
  "version": "1.0.1",
  "permissions": [
    "contextMenus",
    "activeTab",
    "https://www.amazon.de/*",
    "https://www.amazon.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "sanitize-amazon@devloberto",
      "strict_min_version": "91.0",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  }
}
