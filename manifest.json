{
  "manifest_version": 3,
  "name": "Sanitize Amazon",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Sanitize Amazon",
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.amazon.de/*",
    "https://www.amazon.com/*"
  ]
}
